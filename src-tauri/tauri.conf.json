{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "multi-transpose",
    "version": "1.3.1"
  },
  "plugins": {
    "log": {
      "level": "trace",
      "targets": ["logDir", "stdout"],
      "file": {
        "path": "multi-transpose.log",
        "level": "trace",
        "rotation": {
          "max_file_size": 10485760,
          "max_files": 5
        }
      }
    }
  },
  "tauri": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://gist.githubusercontent.com/Albacusphetical/b36c4d985ad1466dbbcfd710e1c6544b/raw/69a928a4792a41f7312c37b0a7e4def7b140e1a2/"
      ],
      "windows": {
        "installMode": "passive"
      },
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDc1MTdFREQ5RkUyQ0YyRTUKUldUbDhpeisyZTBYZFlveVUwVnRPM1J4RDJocWo5QmRaeG1zNUJteHZ5b0lCb29jRlhManhjRi8K"
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "create": true,
        "setTitle": true,
        "setDecorations": true
      },
      "path": {
        "all": true
      },
      "fs": {
        "readFile": true,
        "writeFile": true,
        "exists": true,
        "scope": [
          "$APPDATA/settings.json",
          "$RESOURCE/*"
        ]
      },
      "clipboard": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.multi-transpose",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico",
        "icons/Square30x30Logo.png",
        "icons/Square71x71Logo.png",
        "icons/Square44x44Logo.png",
        "icons/Square89x89Logo.png",
        "icons/Square107x107Logo.png",
        "icons/Square142x142Logo.png",
        "icons/Square150x150Logo.png",
        "icons/Square284x284Logo.png",
        "icons/Square310x310Logo.png",
        "icons/StoreLogo.png"
      ],
      "resources": [
        "assets/*"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "multi-transpose",
        "minWidth": 500,
        "minHeight": 500
      }
    ],
    "macOSPrivateApi": true
  }
}
